{
  "name": "@herrevilkitten/telnet",
  "version": "1.0.3",
  "description": "",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "build": "pnpm clean && pnpm build:esm && pnpm build:cjs",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rimraf dist",
    "test": "vitest --run",
    "test:watch": "vitest",
    "example:server:echo": "tsx examples/server/echo",
    "example:server:daytime": "tsx examples/server/daytime",
    "example:server:chargen": "tsx examples/server/chargen",
    "example:client:daytime": "tsx examples/client/daytime",
    "example:client:http": "tsx examples/client/http",
    "example:client:https": "tsx examples/client/https"
  },
  "keywords": [
    "telnet",
    "generator"
  ],
  "author": "",
  "license": "Apache 2.0",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/node": "^17.0.45",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "^4.9.5",
    "vitest": "^4.0.18"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:herrevilkitten/telnet-generator.git"
  },
  "bugs": {
    "url": "https://github.com/herrevilkitten/telnet-generator/issues"
  },
  "dependencies": {
    "@herrevilkitten/async-queue": "1.0.0"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "importOrder": [
      "^@engine/(.*)$",
      "^@model/(.*)$",
      "^@util/(.*)$",
      "^[./]",
      "^\\./"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderParserPlugins": [
      "typescript"
    ],
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ]
  }
}
